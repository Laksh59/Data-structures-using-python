    "#last index of one\n",
    "x=[0,1,0,1,1,1,0]\n",
    "def move_zero_to_end(x):\n",
    "    ones=x.count(1)\n",
    "    zeroes=x.count(0)\n",
    "    result=[1]*ones+[0]*zeroes\n",
    "    return result\n",
    "print(\"output:\",move_zero_to_end(x))"
   ]
  }
