    "#2.Write a program to find the determinant of a matrix\n",
    "import numpy as np\n",
    "def determinant(matrix):\n",
    "    return int(np.linalg.det(matrix))\n",
    "\n",
    "matrix = [\n",
    "    [1, 2, 3],\n",
    "    [0, 4, 5],\n",
    "    [1, 0, 6]\n",
    "]\n",
    "print(determinant(matrix))"
   ]
  }
